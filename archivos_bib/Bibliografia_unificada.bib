  author = {Siddiqui, Shahida Anusha and Ristow, Bridget and Rahayu, Teguh and Putra, Nugroho Susetya and Widya Yuwono, Nasih and Nisa', Khoirun and Mategeko, Bosco and Smetana, Sergiy and Saki, Morteza and Nawaz, Asad and Nagdalian, Andrey},
