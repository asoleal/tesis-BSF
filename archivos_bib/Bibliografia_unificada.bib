  isbn = {979-8-3503-5059-3},
  langid = {english},
  file = {/home/jjlg/Zotero/storage/UETI7T2M/A et al. - 2024 - AI Controlled Smart IoT Based Greenhouse Monitoring System.pdf}
}

@article{adadiPeekingBlackBoxSurvey2018,
