@article{kamilarisReviewPracticeBig2017,
  title = {A Review on the Practice of Big Data Analysis in Agriculture},
  author = {Kamilaris, Andreas and Kartakoullis, Andreas and {Prenafeta-Bold{\'u}}, Francesc X.},
  year = {2017},
