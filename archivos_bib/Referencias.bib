@article{weichertReviewMachineLearning2019,
  title = {A Review of Machine Learning for the Optimization of Production Processes},
  author = {Weichert, D. and Link, Patrick and Stoll, Anke and R{\"u}ping, S. and Ihlenfeldt, S. and Wrobel, S.},
